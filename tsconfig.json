{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "strict": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "types": ["@nuxt/kit"],
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
  },
  "exclude": [
    "node_modules"
  ]
}
